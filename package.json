        "eslint": "^8.11.0",
        "eslint-config-prettier": "^8.5.0",
        "json-server": "^0.17.0",
        "prettier": "^2.6.0",
        "sass": "^1.49.9",
        "stylelint": "^14.6.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard-scss": "^3.0.0",
        "typescript": "^4.5.4",
        "vite": "^2.8.0",
        "vite-plugin-eslint": "^1.3.0",
        "vite-plugin-stylelint": "^1.2.0"
